#include<stdio.h>
int main()
{
    int month, year;
    
    scanf("%d",&month);
    scanf("%d",&year);
    
    if(month==1 && year>0){
        printf("31");
    }
      else if(month==0 || month>=13 || month<0){
          if(year<=0){
        printf("Invalid Month\n");
           printf("Invalid Year");
      }
          else{
               printf("Invalid Month");
          }
    }
     else if(month==0 || month>=13 || month<0){
          printf("Invalid Month");
     }
    else if(year<=0){
          printf("Invalid Year");
    }
    else if(month==0 || month>=13 || month<0){
          if(year<=0)
        printf("Invalid Month\n");
           printf("Invalid Year");
    }
   else if(month==3 && year>0){
        printf("31");
    }
     else if(month==4 && year>0){
        printf("30");
    }
     else if(month==5 && year>0){
        printf("31");
    }
      else if(month==6 && year>0){
        printf("30");
    }
      else if(month==7 && year>0){
        printf("31");
    }
     else if(month==8 && year>0){
        printf("31");
    }
     else if(month==9 && year>0){
        printf("30");
    }
    else if(month==10 && year>0){
        printf("31");
    }
    else if(month==11 && year>0){
        printf("30");
    }
    else if(month==12 && year>0){
        printf("31");
    }
    else if(month==2){
        if(year%4!=0){
            printf("28");
        }
            else if(year%100==0 && year%400!=0){
                 printf("28");
            }
            else{
                printf("29");
            }
        }
    
    
    
    
    
}
